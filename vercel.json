{
  "version": 2,
  "builds": [
    {
      "src": "api/serverless.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/admin.html",
      "dest": "/public/admin.html"
    },
    {
      "src": "/(.*\\.(css|js|html|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/serverless.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
